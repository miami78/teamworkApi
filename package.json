    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "nodemon": "^1.19.4",
    "pg": "^7.4.0",
    "pg-promise": "^9.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
